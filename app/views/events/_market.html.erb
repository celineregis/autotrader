<div class="col-md-4 market-name-holder">
	<h4><%="#{market} #{param}"%></h4>
</div>
<div class="col-md-8">
	<table class="table table-bordered" id="table-standard-match">
    <tbody>
      <tr>
        <%odds.each_with_index do |odd, index| %>
          <th class="text-center" id=<%="#{market.gsub(/[^0-9A-Za-z]/, '')}-#{param}-#{index}"%>><%=odd.to_f.round(2)%></th>
        <%end%>
      </tr>
      <tr>
        <%odds.each do |odd| %>
          <td class="text-center"><%=(odd.to_f-0.05).round(2)%></th>
        <%end%>
      </tr>
        <%odds.length.times do %>
          <td class="col-md-2">20</td>
        <%end%>
      <tr>
        <%odds.length.times do %>
          <td class="col-md-2">20</td>
        <%end%>
      </tr>
    </tbody>
  </table>
</div>
  







